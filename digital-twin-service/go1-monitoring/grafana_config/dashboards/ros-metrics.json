            "uid": "yBmilKUIz"
          },
          "query": "from(bucket: \"monitoring\")\n  |> range(start: -30m)\n  |> filter(fn: (r) =>\n    r._measurement == \"ros_metrics\" and\n    r.metric_type == \"delay\" and\n    r._field == \"avg_ms\" and\n    contains(value: r.topic, set: [\"/scan\",\"/joint_states\",\"/odometry/filtered\",\"/go1_controller/cmd_vel\"])\n  )\n  |> map(fn: (r) => ({\n    r with topic_name:\n      if r.topic == \"/scan\" then \"Laser Scan (lidar)\"\n      else if r.topic == \"/joint_states\" then \"Joint States (go1-base)\"\n      else if r.topic == \"/odometry/filtered\" then \"Odometry (go1-base)\"\n      else if r.topic == \"/go1_controller/cmd_vel\" then \"Velocity (go1-navigation)\"\n      else r.topic\n  }))\n  |> group(columns: [\"topic_name\"])\n  |> aggregateWindow(every: 5s, fn: mean, createEmpty: false)\n  |> keep(columns: [\"_time\",\"_value\",\"topic_name\"])\n",
          "refId": "A"
        }
      ],
      "title": "Delay by ROS Topic",
      "type": "timeseries"
