          "query": "import \"strings\"\r\n\r\nfrom(bucket: \"monitoring\")\r\n  |> range(start: -15m)\r\n  |> filter(fn: (r) =>\r\n       r._measurement == \"applications\" and\r\n       (r._field == \"net_rx_bytes\" or r._field == \"net_tx_bytes\") and\r\n       contains(value: r.pod, set: ${pod:json})\r\n     )\r\n  |> group(columns: [\"pod\", \"_field\"])\r\n  |> aggregateWindow(every: v.windowPeriod, fn: last, createEmpty: false)\r\n  |> fill(column: \"_value\", usePrevious: true)\r\n  |> derivative(unit: 1s, nonNegative: true)\r\n  |> map(fn: (r) => ({\r\n       _time:  r._time,\r\n       _value: r._value * 8.0,\r\n       _field: r.pod + \" \" + (if r._field == \"net_tx_bytes\" then \"TX\" else \"RX\")\r\n     }))\r\n  |> keep(columns: [\"_time\", \"_field\", \"_value\"])\r\n  |> yield(name: \"pod_network_throughput_bps\")\r\n",
